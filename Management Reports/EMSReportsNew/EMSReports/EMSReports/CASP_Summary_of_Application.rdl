  <DataSets>
    <DataSet Name="CASPSummary">
      <Query>
        <DataSourceName>EMSDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SubmissionStartDate">
            <Value>=Parameters!SubmissionStartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SubmissionEndDate">
            <Value>=Parameters!SubmissionEndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@status">
            <Value>=Parameters!status.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@eligibilitystatus">
            <Value>=Parameters!eligibilitystatus.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select distinct  a.Programme_ID,
[Application_No]
,[Submitted_Date]
,[Company_Project]
,[Accelerator_Name]
,a.Modified_By as Coordinator
,[Commencement_Date]
,[Status]

,(select top 1 convert(nvarchar(2000),Comment_For_Internal_Use) from [TB_SCREENING_HISTORY] where b.Application_Number = Application_Number and b.Programme_ID=Programme_ID and b.Validation_Result=Validation_Result  order by Created_Date desc)  as Internal_comments


from 
[TB_CASP_APPLICATION] a
left join [TB_SCREENING_HISTORY] b on a.Application_No = b.Application_Number and a.Programme_ID=b.Programme_ID and a.status=b.Validation_Result

where 
Submitted_Date &gt;= @SubmissionStartDate and Submitted_Date &lt;= @SubmissionEndDate and
 a.status= Case when @status = 'All' then a.status 
                     when @status ='Saved' then @status
					 end or (a.status &lt;&gt; case when @status ='Submitted' and @eligibilitystatus &lt;&gt;'All' then 'Saved' end and a.status=@eligibilitystatus and b.Validation_Result=@eligibilitystatus) 
					 or (a.status &lt;&gt; case when @status ='Submitted' and @eligibilitystatus ='All' then 'Saved' end)
</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
