  <DataSets>
    <DataSet Name="FundingListData">
      <Query>
        <DataSourceName>EMSDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Programme_Type">
            <Value>=Parameters!Programme_Type.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @Programme_Type varchar(50)
--set @Programme_Type ='all'

SELECT distinct
cf.created_date,
 cp.Company_Name
 ,case @Programme_Type 
         WHEN 'ALL'  then  CASE cf.created_Date
				WHEN  (Select top 1 created_Date from [TB_COMPANY_FUND] where Company_Profile_ID=cm.Company_Profile_ID  and Programme_Type is null)
				THEN '1'
				ELSE NULL
				end
		 WHEN 'CCMF'  then  CASE cf.created_Date
				WHEN  (Select top 1 created_Date from [TB_COMPANY_FUND] c
				where c.Company_Profile_ID=cm.Company_Profile_ID and cm.Applicaition_Type='CCMF'  and c.Programme_Type is null)
				THEN '1'
				ELSE NULL
				end

		WHEN 'CPIP'  then  CASE cf.created_Date
				WHEN (Select top 1 created_Date from [TB_COMPANY_FUND] c
				where c.Company_Profile_ID=cm.Company_Profile_ID and cm.Applicaition_Type='CPIP' and  c.Programme_Type is null)
				THEN '1'
				ELSE NULL
				end

          ELSE NULL
       END AS Unique_Company
,cf.Programme_Type
,CASE cf.Programme_Type
         WHEN  'CCMF' THEN ccmfApp.Intake_Number
         WHEN  'CPIP' THEN cpipApp.Intake_Number
		 else Null
       END AS Intake_Number
,cf.Programme_Name
,cf.Currency
,cf.Amount_Received
,cf.Invertor_Info
,cf.Funding_Origin
,cf.Application_Status
,cf.Received_Date
,cf.Reported_Date
,cf.Remarks
  FROM [TB_COMPANY_FUND] cf
  left join TB_COMPANY_PROFILE_BASIC cp on cf.Company_Profile_ID = cp.Company_Profile_ID
   left join TB_COMPANY_APPLICATION_MAP cm on cp.Company_Profile_ID=cm.Company_Profile_ID
  Left join TB_CCMF_APPLICATION ccmfApp on ccmfApp.Application_Number=cf.Application_No 
  Left join TB_INCUBATION_APPLICATION cpipApp on cpipApp.Application_Number=cf.Application_No 
  where 
  cf.Programme_Type is null-- or cf.Programme_Type=@Programme_Type
  and
    ( @Programme_Type = 'All' and cm.Applicaition_Type in ('CCMF','CPIP')
or @Programme_Type = 'CCMF' and cm.Applicaition_Type = 'CCMF'
or @Programme_Type = 'CPIP' and cm.Applicaition_Type = 'CPIP'
) 
order by cp.Company_Name</CommandText>
