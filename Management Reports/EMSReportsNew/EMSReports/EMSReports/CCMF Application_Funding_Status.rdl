  <DataSets>
    <DataSet Name="CCMFRecords">
      <Query>
        <DataSourceName>EMSDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IntakeNumber">
            <Value>=Parameters!IntakeNumber.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ApplicationNumber">
            <Value>=Parameters!ApplicationNumber.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ProjectName">
            <Value>=Parameters!ProjectName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @IntakeNumber varchar(10)
--declare @ApplicationNumber varchar(50)
--declare @ProjectName varchar(250)
--set @IntakeNumber='201706'
--set @ApplicationNumber = 'CCMF-201804-0003'

select 
Application_Number
,Intake_Number
,cf.Programme_Name
,ca.Project_Name_Eng
,ca.Project_Name_Chi
,cf.Date
,cf.Application_Status
,cf.Funding_Status
,cf.Expenditure_Nature
,cp.Company_Name
,cf.Currency
,cf.Amount_Received
,cf.Maximum_Amount

from TB_APPLICATION_FUNDING_STATUS cf
inner join TB_CCMF_APPLICATION ca on cf.Application_ID=ca.CCMF_ID
Left join TB_COMPANY_APPLICATION_MAP  cm on ca.Application_Number = cm.Application_No 
Left join TB_COMPANY_PROFILE_BASIC cp on cm.Company_Profile_ID = cp.Company_Profile_ID

where ca.Intake_Number = @IntakeNumber and (ca.Application_Number = @ApplicationNumber OR @ApplicationNumber IS NULL) 
and (ca.Project_Name_Eng = @ProjectName OR @ProjectName IS NULL) and ca.Status not in('Saved','Disqualified','Deleted')
order by ca.Application_Number

</CommandText>
      </Query>
