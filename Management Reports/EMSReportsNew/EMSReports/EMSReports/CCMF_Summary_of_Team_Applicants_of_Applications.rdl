  <DataSets>
    <DataSet Name="CCMF_Summary">
      <Query>
        <DataSourceName>EMSDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IntakeNumber">
            <Value>=Parameters!IntakeNumber.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ApplicationNumber">
            <Value>=Parameters!ApplicationNumber.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ProjectName">
            <Value>=Parameters!ProjectName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EligibilityStatus">
            <Value>=Parameters!EligibilityStatus.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>If(OBJECT_ID('tempdb..#TB_CCMFAPPLICATION') Is Not Null)
Drop Table #TB_CCMFAPPLICATION

If(OBJECT_ID('tempdb..#TB_SCREENING_Comments') Is Not Null)
Drop Table #TB_SCREENING_Comments

--Declare @IntakeNumber varchar(50)
--Declare @EligibilityStatus varchar(200)
--declare @ApplicationNumber varchar(50)
--declare @ProjectName varchar(200)

--set @IntakeNumber=201706
--set @EligibilityStatus='All'
----set @ApplicationNumber ='CCMF-201706-0885'
--set @ProjectName ='Job'
 
 select distinct tc.Application_Number,tc.Programme_ID
 into #TB_CCMFAPPLICATION
 from TB_PROGRAMME_INTAKE tb  
 inner join  TB_CCMF_APPLICATION tc on tb.Programme_ID=tc.Programme_ID
 inner join TB_SCREENING_CCMF_SCORE ts on tc.Application_Number=ts.Application_Number and tc.Programme_ID=ts.Programme_ID
 where  tb.Intake_Number =@IntakeNumber
  and tc.Application_Number= Case when COALESCE(@ApplicationNumber,'')=''   then tc.Application_Number else @ApplicationNumber end 


  
SELECT a.Application_Number,a.Programme_ID, STUFF((
    select ','+ b.Role + ':'+ Comments
    from TB_SCREENING_CCMF_SCORE b
    WHERE a.Application_Number = b.Application_Number
	and  (b.Role  like '%BDM%' or  b.Role  = 'CPMO' or b.Role  = 'Senior Manager')
    FOR XML PATH('')
    )
    ,1,1,'') AS Comments
	into #TB_SCREENING_Comments
FROM TB_SCREENING_CCMF_SCORE a
inner join #TB_CCMFAPPLICATION b on a.Application_Number=b.Application_Number and a.Programme_ID=b.Programme_ID
GROUP BY a.Application_Number,a.Programme_ID



select
distinct
 tc.Application_Number
,tc.Hong_Kong_programme_Stream
,tc.CCMF_Application_Type
,tc.Project_Name_Eng
,tc.CCMF_ID 
,tc.Advisor_Info
,tc.Principal_Full_Name
,tc.Principal_Position_Title
,tc.Marketing_Information
,ta.Name,ta.Position
,ta.Email
--,ts.Role
,CAST(ta.Background_Information AS NVARCHAR(MAX)) as Background_Information
,ts.Comments
,ta.Masked_HKID
,ta.Application_ID
,tc.Status

 from TB_PROGRAMME_INTAKE tb  
 inner join  TB_CCMF_APPLICATION tc on tb.Programme_ID=tc.Programme_ID
 inner join TB_APPLICATION_COMPANY_CORE_MEMBER ta on tc.CCMF_ID = ta.Application_ID
 Left join #TB_SCREENING_Comments ts on tc.Application_Number=ts.Application_Number and tc.Programme_ID=ts.Programme_ID

 where 
-- tc.status not in('Saved','Deleted')
 tb.Intake_Number =@IntakeNumber 
 
 --and  (ts.Role  like '%BDM%' or  ts.Role  = 'CPMO' or ts.Role  = 'Senior Manager')
 and tc.Application_Number= Case when COALESCE(@ApplicationNumber,'')=''   then tc.Application_Number else @ApplicationNumber end
 and tc.Project_Name_Eng like Case when COALESCE(@ProjectName,'') ='' then tc.Project_Name_Eng else @ProjectName +'%' end
 and  
 ( @EligibilityStatus = 'All' and tc.status = tc.status or
  @EligibilityStatus = 'CPMO Reviewed/ Complete Screening' and tc.status IN ('Complete Screening','CPMO Reviewed') or
 @EligibilityStatus = tc.status)

order by tc.Application_Number</CommandText>
