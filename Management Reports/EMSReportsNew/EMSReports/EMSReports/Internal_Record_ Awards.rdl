    <DataSet Name="ClusterData">
      <Query>
        <DataSourceName>EMSDataSource</DataSourceName>
        <CommandText>IF OBJECT_ID('tempdb.dbo.#ClusterData', 'U') IS NOT NULL
  DROP TABLE #ClusterData; 
Create Table #ClusterData (
    Cluster varchar(250)
);

Insert Into #ClusterData values(null)
Insert Into #ClusterData values('App Design/ Web Production')
Insert Into #ClusterData values('E-Commerce')
Insert Into #ClusterData values('Edutech')
Insert Into #ClusterData values('AI / Big Data')
Insert Into #ClusterData values('Fintech')
Insert Into #ClusterData values('Gaming')
Insert Into #ClusterData values('Healthtech')
Insert Into #ClusterData values('Wearable / IoT')
Insert Into #ClusterData values('Social Media')
Insert Into #ClusterData values('Others')
select Cluster from #ClusterData</CommandText>
      </Query>
      <Fields>
        <Field Name="Cluster">
          <DataField>Cluster</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
