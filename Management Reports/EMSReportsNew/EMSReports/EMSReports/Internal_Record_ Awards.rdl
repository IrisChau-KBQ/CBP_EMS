    <DataSet Name="InternalAwards">
      <Query>
        <DataSourceName>EMSDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDateYear">
            <Value>=Parameters!StartDateYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDateMonth">
            <Value>=Parameters!StartDateMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyName">
            <Value>=Parameters!CompanyName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDateYear">
            <Value>=Parameters!EndDateYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDateMonth">
            <Value>=Parameters!EndDateMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Nature">
            <Value>=Parameters!Nature.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyCluster">
            <Value>=Parameters!CompanyCluster.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AwardCluster">
            <Value>=Parameters!AwardCluster.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @StartDateMonth varchar(2)
--declare @StartDateYear varchar(4)
--declare @EnsDateMonth varchar(2)
--declare @EndDateYear varchar(4)
--declare @CompanyName varchar(200)
--declare @AwardCluster varchar(250)
--declare @CompanyCluster varchar(250)
--declare @Nature varchar(6)

--set @StartDateMonth='06'
--set @StartDateYear='2018'
--set @EnsDateMonth='08'
--set @EndDateYear='2018'
--set @CompanyCluster ='Gaming'
--set @AwardCluster ='Gaming'

--set @DateRange = '201808'
--set @CompanyName='CCMF Project 5'
--set @Nature ='CCMF'


select distinct
--ROW_NUMBER() Over (Order by a.Company_Profile_ID) As [Number]
b.Nature as AwardNature
,(select top 1 Applicaition_Type             
           from TB_COMPANY_APPLICATION_MAP  where Company_Profile_ID=a.Company_Profile_ID order by Created_Date desc) as Application_Type
,b.Awarded_Year_Month

, (select top 1 CASE Applicaition_Type 
             WHEN 'CCMF' THEN 1
             ELSE NULL
           END from TB_COMPANY_APPLICATION_MAP  where Company_Profile_ID=a.Company_Profile_ID order by Created_Date desc)  AS CCMFType

, (select top 1 CASE Applicaition_Type 
             WHEN 'CPIP' THEN 1
             ELSE NULL
           END from TB_COMPANY_APPLICATION_MAP  where Company_Profile_ID=a.Company_Profile_ID order by Created_Date desc)  AS CPIPType
,a.Company_Name
,'' as Product

,CASE b.Type
    WHEN 'Local' THEN 1
    ELSE NULL
END AS Local

,CASE b.Type
    WHEN 'Overseas' THEN 1
    ELSE NULL
END AS Overseas

,CASE b.Type
    WHEN 'Mainland' THEN 1
    ELSE NULL
END AS Mainland
,b.Name
,b.Product_Name
,b.Award_Name
,a.CCMF_Custer as Main_Cluster
,a.CPIP_Custer as second_Cluster
,b.Remarks as Input_by_staff

from TB_company_Award  b

inner join TB_COmpany_Profile_Basic a on b.Company_Profile_ID = a.Company_Profile_ID
inner join TB_COMPANY_APPLICATION_MAP c on b.Company_Profile_ID = c.Company_Profile_ID

where  
 ((convert(varchar(6), Awarded_Year_Month, 112) &gt;=  @StartDateYear+@StartDateMonth
AND convert(varchar(6), Awarded_Year_Month, 112) &lt;= @EndDateYear+@EndDateMonth) or (@StartDateYear is null and @StartDateMonth is null and @EndDateYear is null and @EndDateMonth is null)) and
 (a.Company_Name =@CompanyName OR @CompanyName IS NULL)
and (b.Nature =@Nature OR @Nature IS NULL)
and (@CompanyCluster = (case when c.Applicaition_Type='CCMF' then a.CCMF_Custer when c.Applicaition_Type='CPIP' then  a.CPIP_Custer  end) or @CompanyCluster is null)
 and (@AwardCluster = (case when b.Nature='CCMF' then a.CCMF_Custer when b.Nature='CPIP' then a.CPIP_Custer	end ) OR @AwardCluster IS NULL)



</CommandText>
      </Query>
