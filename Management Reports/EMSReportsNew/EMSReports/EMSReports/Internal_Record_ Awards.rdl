    <DataSet Name="CompanyName">
      <Query>
        <DataSourceName>EMSDataSource</DataSourceName>
        <CommandText>select null as Company_Name
union all
select distinct Company_Name as Company_Name from TB_COmpany_Profile_Basic a
inner join TB_company_Award b on  a.Company_Profile_ID = b.Company_Profile_ID</CommandText>
      </Query>
