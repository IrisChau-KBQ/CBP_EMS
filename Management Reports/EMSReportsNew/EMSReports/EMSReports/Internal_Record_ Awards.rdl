    <DataSet Name="DatesData">
      <Query>
        <DataSourceName>EMSDataSource</DataSourceName>
        <CommandText>DECLARE @dt1 int='2005'
DECLARE @dt2 int='2055'


;WITH ctedaterange 
     AS (select null as dates
UNION ALL
	 SELECT [Dates]=@dt1 
         UNION ALL
         SELECT [dates] + 1 
         FROM   ctedaterange 
         WHERE  [dates] + 1&lt;= @dt2) 


SELECT [dates] 
FROM   ctedaterange 
OPTION (maxrecursion 0)</CommandText>
      </Query>
      <Fields>
        <Field Name="dates">
          <DataField>dates</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
